{
  "type": "View?id=sidebar;style:[minWidth=4.5rem;maxWidth=4.5rem;width=4.5rem;backgroundColor=#f6f6f6;height=100%]",
  "id": "sidebar",
  "children": [
    {
      "controls": [
        {
          "event": "click?close-flights();close-invoices();deselect-sidebar-btns();open-manager():[manager=dashboard-manager]"
        }
      ],
      "type": "View?id=dashboard-sidebar-btn;tooltip.text=Dashboard;opened-manager:()=dashboard-manager;class=flexbox pointer;style():[if():[!agent:().admin]:[display=none];height=5rem;transition=.2s;borderRight=if():[opened-manager:()=dashboard-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
      "children": [
        {
          "type": "Icon?name=speed;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=dashboard-manager]:blue:#888]"
        }
      ]
    },
    {
      "children": [
        {
          "type": "Icon?name=flight_takeoff;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=book-a-flight-manager]:blue:#888]"
        },
        {
          "controls": [
            {
              "event": "click?if():[flights-opened:()]:[close-expand-more-flight()]:[open-expand-more-flight()]"
            }
          ],
          "children": [
            {
              "type": "Icon?name=expand_more;google.outlined;style:[fontSize=2rem;transition=.2s]"
            }
          ],
          "type": "View:flight-expand-more?class=flexbox;style:[width=0;height=3rem;borderRadius=0 .5rem .5rem 0;pointerEvents=none;position=absolute;right=0;transition=.2s;cursor=pointer;zIndex=1;backgroundColor=#e6e6e6;overflow=hidden]"
        }
      ],
      "controls": [
        {
          "event": "click?close-flights();close-invoices();deselect-sidebar-btns();open-manager():[manager=book-a-flight-manager]?!2ndChild().contains():[clicked:()]"
        },
        {
          "event": "mouseenter?clearTimer():[().expand-timer];2ndChild().style():[width=2.5rem;right=-2.8rem;pointerEvents=auto]"
        },
        {
          "event": "mouseleave?expand-timer=timer():[if():[!mouseenter]:[2ndChild().style():[width=0;right=0;pointerEvents=none]]]:200"
        }
      ],
      "type": "View?tooltip.text=book a flight seat;class=flexbox pointer;style:[height=5rem;minWidth=100%;minHeight=5rem;transition=.2s;position=relative;borderRight=if():[opened-manager:()=book-a-flight-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6"
    },
    {
      "type": "View?id=my-flights;class=vertical;style:[width=4.5rem;height=0;transition=.2s;overflow=hidden]",
      "children": [
        {
          "type": "View?id=flight-sidebar-btn;tooltip.text=flights;class=flexbox pointer;style:[if():[!user:().admin||!agent:().admin]:[display=none];height=5rem;minHeight=5rem;flex=1;transition=.2s;position=relative;borderRight=if():[opened-manager:()=flights-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
          "controls": [
            {
              "event": "click?close-invoices();deselect-sidebar-btns();deselect-flight-btns();open-expand-more-flight();open-manager():[manager=flights-manager]"
            }
          ],
          "children": [
            {
              "type": "Icon?name=flight;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=flights-manager]:blue:#888;transition=.4s]"
            }
          ]
        },
        {
          "type": "View?id=flight-booking-sidebar-btn;tooltip.text=my bookings;class=flexbox pointer;style:[height=5rem;minHeight=5rem;transition=.2s;flex=1;borderRight=if():[opened-manager:()=bookings-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
          "controls": [
            {
              "event": "click?close-invoices();deselect-sidebar-btns();deselect-flight-btns();open-expand-more-flight();open-manager():[manager=bookings-manager]"
            }
          ],
          "children": [
            {
              "type": "Icon?name=backpack;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=flights-manager]:blue:#888]"
            }
          ]
        },
        {
          "controls": [
            {
              "event": "click?close-invoices();deselect-sidebar-btns();deselect-flight-btns();open-expand-more-flight();open-manager():[manager=tickets-manager]"
            }
          ],
          "children": [
            {
              "type": "Icon?name=airplane_ticket;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=flights-manager]:blue:#888]"
            }
          ],
          "type": "View?id=flight-ticket-sidebar-btn;tooltip.text=my tickets;class=flexbox pointer;style:[height=5rem;minHeight=5rem;transition=.2s;flex=1;borderRight=if():[opened-manager:()=tickets-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6"
        },
        {
          "type": "View?class=flexbox;style:[width=90%;height=1px;marginLeft=5%;borderBottom=1px solid #ddd]"
        }
      ]
    },
    {
      "type": "View:hotels-btn?tooltip.text=book a hotel room;class=flexbox pointer column;style:[height=5rem;minHeight=5rem;transition=.2s;position=relative;borderRight=if():[opened-manager:()=book-a-hotel-manager]:2px solid #FF4340:2px solid #00000000];hover.style.backgroundColor=#e6e6e6?false",
      "children": [
        {
          "type": "View?class=flexbox;style.gap=.1rem",
          "children": [
            {
              "type": "Icon?name=bi-star-fill;style:[color=#888;fontSize=.5rem;transition=.2s]"
            },
            {
              "type": "Icon?name=bi-star-fill;style:[color=#888;fontSize=.5rem;transition=.2s]"
            },
            {
              "type": "Icon?name=bi-star-fill;style:[color=#888;fontSize=.5rem;transition=.2s]"
            }
          ]
        },
        {
          "type": "Icon?name=apartment;google;outlined;style:[color=#888;fontSize=2.7rem;transition=.2s]"
        }
      ],
      "controls": [
        {
          "event": "click"
        }
      ]
    },
    {
      "type": "View?id=my-hotels;class=vertical;style:[width=4.5rem;height=0;transition=.2s;overflow=hidden]"
    },
    {
      "children": [
        {
          "type": "Icon?name=shopping_cart;google.outlined;style:[fontSize=2.8rem;color=if():[opened-manager:()=invoices-manager]:blue:#888;transition=.4s]"
        },
        {
          "type": "View:invoice-expand-more?class=flexbox;style:[width=0;height=3rem;borderRadius=0 .5rem .5rem 0;pointerEvents=none;position=absolute;right=0;transition=.2s;cursor=pointer;zIndex=1;backgroundColor=#e6e6e6;overflow=hidden]",
          "children": [
            {
              "type": "Icon?name=expand_more;google.outlined;style:[fontSize=2rem;transition=.2s]"
            }
          ],
          "controls": [
            {
              "event": "click?if():[invoices-opened:()]:[close-expand-more-invoice()]:[open-expand-more-invoice()]"
            }
          ]
        }
      ],
      "type": "View:invoice-sidebar-btn?tooltip.text=my transactions;class=flexbox pointer;style:[height=5rem;minWidth=100%;minHeight=5rem;transition=.2s;position=relative;borderRight=if():[opened-manager:()=invoices-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
      "controls": [
        {
          "event": "click?close-invoices();close-flights();deselect-sidebar-btns();open-manager():[manager=invoices-manager]?!2ndChild().contains():[clicked:()]"
        },
        {
          "event": "mouseenter?clearTimer():[().expand-timer];2ndChild().style():[width=2.5rem;right=-2.8rem;pointerEvents=auto]"
        },
        {
          "event": "mouseleave?expand-timer=timer():[if():[!mouseenter]:[2ndChild().style():[width=0;right=0;pointerEvents=none]]]:200"
        }
      ]
    },
    {
      "type": "View?id=my-invoices;class=vertical;style:[width=4.5rem;height=0;transition=.2s;overflow=hidden]",
      "children": [
        {
          "children": [
            {
              "type": "Icon?name=account_balance_wallet;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=deposits-manager]:blue:#888]"
            }
          ],
          "type": "View?id=deposit-sidebar-btn;tooltip.text=my deposits;class=flexbox pointer;style:[height=5rem;minHeight=5rem;transition=.2s;flex=1;borderRight=if():[opened-manager:()=deposits-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6?false",
          "controls": [
            {
              "event": "click?close-flights();deselect-sidebar-btns();deselect-invoice-btns();open-expand-more-invoice();open-manager():[manager=deposits-manager]"
            }
          ]
        },
        {
          "children": [
            {
              "type": "Icon?name=badge;google.outlined;style:[fontSize=2.5rem;color=if():[opened-manager:()=agents-manager]:blue:#888]"
            }
          ],
          "type": "View?id=agent-sidebar-btn;tooltip.text=my agents;class=flexbox pointer;style:[if():[!agent:().admin||!user:().admin]:[display=none];height=5rem;minHeight=5rem;transition=.2s;flex=1;borderRight=if():[opened-manager:()=agents-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
          "controls": [
            {
              "event": "click?close-flights();deselect-sidebar-btns();deselect-invoice-btns();open-expand-more-invoice();open-manager():[manager=agents-manager]"
            }
          ]
        },
        {
          "controls": [
            {
              "event": "click?close-flights();deselect-sidebar-btns();deselect-invoice-btns();open-expand-more-invoice();open-manager():[manager=users-manager]"
            }
          ],
          "type": "View?id=user-sidebar-btn;tooltip.text=my users;class=flexbox pointer;style:[height=5rem;minHeight=5rem;transition=.2s;flex=1;borderRight=if():[opened-manager:()=users-manager]:2px solid blue:2px solid #00000000];hover.style.backgroundColor=#e6e6e6",
          "children": [
            {
              "type": "Icon?name=account_circle;google.outlined;style:[fontSize=2.6rem;color=if():[opened-manager:()=flights-manager]:blue:#888]"
            }
          ]
        },
        {
          "type": "View?class=flexbox;style:[width=90%;height=1px;marginLeft=5%;borderBottom=1px solid #ddd]"
        }
      ]
    }
  ],
  "functions": {
    "close-hotels": "hotels-opened:()=false;close-expand-more-hotel();deselect-hotel-btns()",
    "open-manager": "toggleView():[parent=manager-view;view=loading-manager;timer=200];if():[opened-manager:()!=_.manager]:[opened-manager:()=_.manager;style():[borderRight=2px solid blue];1stChild().style():[color=blue];hover.disable=true;toggleView():[parent=manager-view;view=_.manager;timer=200]]:[opened-manager:().del()]",
    "open-expand-more-invoice": "():my-invoices.style().height=if():[user:().admin;agent:().admin]:[10.1rem]:[10.1rem];():[invoice-expand-more].1stChild().style().transform=rotate(180deg);invoices-opened:()=true",
    "close-expand-more-flight": "():my-flights.style().height=0;():[flight-expand-more].1stChild().style().transform=rotate(0);flights-opened:()=false",
    "close-expand-more-invoice": "():my-invoices.style().height=0;():[invoice-expand-more].1stChild().style().transform=rotate(0);invoices-opened:()=false",
    "close-invoices": "invoices-opened:()=false;close-expand-more-invoice();deselect-invoice-btns()",
    "deselect-hotel-btns": "():my-hotels.children().():[style():[backgroundColor=inherit;borderRight=2px solid #00000000];1stChild().style().color=#888]",
    "deselect-sidebar-btns": "():sidebar.children().pullItems():[id=my-flights||id=my-hotels||id=my-invoices].():[style():[backgroundColor=inherit;borderRight=2px solid #00000000];1stChild().style().color=#888;hover.disable=false]",
    "deselect-flight-btns": "():my-flights.children().():[style():[backgroundColor=inherit;borderRight=2px solid #00000000];1stChild().style().color=#888]",
    "deselect-invoice-btns": "():my-invoices.children().():[style():[backgroundColor=inherit;borderRight=2px solid #00000000];1stChild().style().color=#888]",
    "close-flights": "flights-opened:()=false;close-expand-more-flight();deselect-flight-btns()",
    "open-expand-more-flight": "():my-flights.style().height=if():[user:().admin;agent:().admin]:[15.1rem]:[10.1rem];():[flight-expand-more].1stChild().style().transform=rotate(180deg);flights-opened:()=true"
  }
}